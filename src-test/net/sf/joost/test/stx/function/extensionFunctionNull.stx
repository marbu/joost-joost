<?xml version="1.0"?>
<stx:transform xmlns:stx="http://stx.sourceforge.net/2002/ns"
               xmlns:t="java:net.sf.joost.test.stx.function.ExtensionFunctionTarget"
               version="1.0">

  <stx:param name="target" required="yes" />
  
  <stx:variable name="value" />

  <stx:template match="/">
    <stx:if test="t:setIntValue($target, $value)" />
    <stx:if test="t:setIntegerValue($target, $value)" />
  </stx:template>

</stx:transform>